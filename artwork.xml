<?xml version="1.0" encoding="UTF-8"?>
<!-- This is the default 'artwork.xml'. Be sure to check the full artwork documentation at https://github.com/muldjord/skyscraper/blob/master/ARTWORK.md -->
<artwork>
  <output type="screenshot" width="640" height="480">
    <layer resource="screenshot" width="520" height="390" align="right" valign="middle">
      <rounded radius="5"/>
      <stroke width="5"/>
    </layer>
    <layer resource="wheel" width="200" align="right">
      <shadow distance="5" softness="3" opacity="65"/>
    </layer>
    <layer resource="cover" width="200" height="200" y="-5" valign="bottom">
      <mask file="mask1.png"/>
      <frame file="frame1.png"/>
      <shadow distance="5" softness="3" opacity="65"/>
    </layer>
  </output>
  <output type="wheel" width="350"/>
  <output type="cover" height="350"/>
  <output type="marquee" width="350"/>
</artwork>
